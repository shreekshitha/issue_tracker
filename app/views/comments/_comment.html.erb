<%= div_for (comment) do %>
<div class="comments_wrapper clearfix">
<div class="pull-left">
<p class="lead"> <%= comment.body %> </p>
<p><small>Submitted <strong><%= time_ago_in_words(comment.created_at) %> ago </strong> </small></p>

<div class="btn-group pull-right">
<% if comment.user == current_user %>
	
<% end %>
</div>
</div>
<% end %>
